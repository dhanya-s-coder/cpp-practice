#include <iostream>
using namespace std;
//jahan jahan here likha hai wahan wahan this pointer use ho skta tha
class Node{
    public:
    int data;
    Node* next;
    Node(){
        data=0;
        this->next=NULL;
    }
    Node(int data){
        this->data=data;//agar node(int value) kr de tioh this lagane ki need nhi hai
        this->next=NULL;
    }
};
class List{
    Node *head;
    public:
    List(){
        head=NULL;//here
        
    }
    void insertAtHead(int data);
    void printList();
    void DeleteList();
};
void List::insertAtHead(int data){
    Node *newNode = new Node(data);
    if(head==NULL){//here
        head=newNode;//here
        return;
    }
    newNode->next=this->head;//yahan this lagao ya n algaoa frak hgi padrga kyuki iss naam ka koi local variable nhi hai
    this->head=newNode;//same here
}
void List::printList(){
    Node* temp;
    temp=head;//here
    if(!this->head){//here
        cout<<"empty\n";
    }
    while(temp!=NULL){
        cout<<temp->data<<" ";
        temp=temp->next;
    }
    cout<<"\n";
}
int main() {
    List l1;
    List l2;
    for(int i=1;i<=5;i++){
        l1.insertAtHead(i);
    }
    l1.printList();
    l2.insertAtHead(1);
    l2.printList();// prints 20
}
