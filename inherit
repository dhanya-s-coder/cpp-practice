#include<iostream>
using namespace std;
class base{
    protected:
    int i,j;
    public:
    void set(int a,int b){
        i=a;
        j=b;
    }
    void show(){
        cout<<i<<" "<<j<<"\n";
    }
};
class derived:public base{
    int k;
    public:
    void setk(){
        k=i*j;
        
    }
    void showk(){
        cout<<k<<endl;
    }
    
};
int main(){
    base b;
    b.set(100,20);
    b.show();
    derived ob;
    ob.set(10,20);
    ob.show();
    ob.setk();
    ob.showk();
}
